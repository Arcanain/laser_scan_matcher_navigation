<launch>
 
  <!-- map1 -->
  <node pkg="map_server" type="map_server" name="map1_server" args="$(find laser_scan_matcher_navigation)/maps/map1.yaml" output="screen">    
    <!-- Topic Name -->
    <remap from="map" to="/map1" />
    <remap from="map_metadata" to="/map1_metadata" />
    <!-- Frame Name -->
    <param name="frame_id" value="map" />
  </node>

  <!-- map2 -->
  <node pkg="map_server" type="map_server" name="map2_server" args="$(find laser_scan_matcher_navigation)/maps/map2.yaml" output="screen">    
